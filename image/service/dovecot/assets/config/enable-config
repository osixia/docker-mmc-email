#!/bin/bash -e

# Usage :
# /osixia/dovecot/config/enable-config [config1] [config2] ...

for i in $@
do

  if [ -f /osixia/dovecot/config/config-available/$i/container-start.sh ]; then
    /osixia/dovecot/config/config-available/$i/container-start.sh
  else
    echo "enable-config: dovecot config $i not found."
    exit 1
  fi
done
