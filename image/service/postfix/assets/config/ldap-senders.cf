server_host = {{ MMC_MAIL_LDAP_URL }}
search_base = ou=Users,{{ MMC_MAIL_LDAP_BASE_DN }}
query_filter = (|(&(objectClass=mailAccount)(mailenable=OK)(|(mail=%s)(mailalias=%s)))(&(objectClass=mailAlias)(mailalias=%s)(mailenable=OK)))
result_attribute = uid
special_result_attribute = mailaliasmember
version = 3
